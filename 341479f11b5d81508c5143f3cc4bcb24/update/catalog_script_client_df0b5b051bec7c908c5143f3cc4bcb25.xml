        <name>Show Security roless options</name>
        <order/>
        <published_ref/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading) {
    if (newValue == '') {
        return;
    }
    var catalogUtils = new GlideAjax('profile');
    catalogUtils.addParam('sysparm_name', 'securityRoles');
    catalogUtils.addParam('sysparm_user_name', newValue);
    catalogUtils.getXML(ajaxResponse);

    function ajaxResponse(serverResponse) {

        var result = serverResponse.responseXML.getElementsByTagName("result");

        var security_role1 = result[0].getAttribute("data");

        var cn = security_role1.split(',');
        for (var i = 0; i < cn.length; i++) {
            var temp = cn[i];
            if (temp.includes('#$')) {
                temp = temp.replace('#$', ',');
            }
            g_form.clearValue('security_role1');
            g_form.addOption("security_role1", temp, temp);
        }

    }
}]]></script>
