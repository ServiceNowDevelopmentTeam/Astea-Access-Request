        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading) {
	if (newValue == '') {
		return;
	}


	//Type appropriate comment here, and begin script below
	g_form.setDisplay('temporary_password', true);
	g_form.setDisplay('user_name_astea',true);
	if (newValue == '1') {
		if(g_form.getValue('state')=='2'){//state is work in progress
			g_form.setMandatory('temporary_password', true);
			g_form.setMandatory('user_name_astea',true);
		}
	} else {
		g_form.setDisplay('temporary_password', false);
		g_form.setDisplay('user_name_astea',false);

		g_form.setMandatory('temporary_password', false);
		g_form.setMandatory('user_name_astea',false);
	}
}]]></script>
